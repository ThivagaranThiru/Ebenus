        <servlet-name>AllCommandesServlet</servlet-name>
        <url-pattern>/AllCommandesServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DetailsCommandeServlet</servlet-name>
        <url-pattern>/DetailsCommandeServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>LoginServlet</welcome-file>
    </welcome-file-list>
