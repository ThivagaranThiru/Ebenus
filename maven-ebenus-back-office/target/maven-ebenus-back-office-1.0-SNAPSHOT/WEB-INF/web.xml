        <servlet-class>com.cours.ebenus.servlets.login.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashboardServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.dashboard.DashboardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllUsersServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.user.AllUsersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddUserServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.user.AddUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateUserServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.user.UpdateUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateAdresseLivraison</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.user.UpdateAdresseLivraison</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AllRolesServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.role.AllRolesServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddRoleServlet</servlet-name>
        <servlet-class>com.cours.ebenus.servlets.role.AddRoleServlet</servlet-class>
